package edu.kh.project.main.controller;

import org.springframework.stereotype.Controller;

// @Controller : 현재 클래스가 컨트롤러임을 명시 -> 요청 응답처리
//				 + Bean 등록

// IOC 제어 역전
// -> 프레임 워크가 객체를 생성하고 관리
// -> 생성된 객체 = Bean

@Controller
public class MainController {

	
	public String mainForward() {
	
		return "common/main";
	}
}
